         <font>
            <Font name="Arial Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btnCancelar" layoutX="240.0" layoutY="455.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="54.0" prefWidth="96.0" style="-fx-background-color: white;" text="Cancelar">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
