      </ImageView>
      <TextField fx:id="tfDistribuidorId" editable="false" layoutX="32.0" layoutY="155.0" maxHeight="1.0" minHeight="30.0" prefHeight="1.0" prefWidth="180.0" style="-fx-background-color: white;" />
      <TextField fx:id="tfNomDistribuidor" layoutX="32.0" layoutY="218.0" maxHeight="1.0" minHeight="30.0" prefHeight="1.0" prefWidth="180.0" style="-fx-background-color: white;" />
      <TextField fx:id="tfTelDistribuidor" layoutX="32.0" layoutY="283.0" maxHeight="1.0" minHeight="30.0" prefHeight="1.0" prefWidth="180.0" style="-fx-background-color: white;" />
      <TextField fx:id="tfDirDistribuidor" layoutX="32.0" layoutY="344.0" maxHeight="1.0" minHeight="30.0" prefHeight="1.0" prefWidth="180.0" style="-fx-background-color: white;" />
